<header>
    <section class="row mb-3">
        {{#with navigation}}
        {{#each links}}
        <div class="col-2">
            <div class="nav nav-tabs">
                <a href="{{href}}" class="nav-link {{#if @last}}disabled{{/if}}">{{text}}</a>
            </div>
        </div>
        {{/each}}
        {{/with}}
    </section>
</header>